<Router AppAssembly="typeof(Startup).Assembly" >
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(ServerSideDebug.Pages.MainLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(ServerSideDebug.Pages.MainLayout)">
            <p>Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
